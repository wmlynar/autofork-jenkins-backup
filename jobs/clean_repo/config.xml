  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers>
    <hudson.triggers.TimerTrigger>
      <spec>3 0 * * 1</spec>
    </hudson.triggers.TimerTrigger>
  </triggers>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>rm -r /var/lib/jenkins/.m2/repository || true
mkdir ros2java
cd ros2java
git clone https://github.com/wmlynar/ros2_java_di.git
git clone https://github.com/wmlynar/ros2_java_di_example.git
git clone https://github.com/wmlynar/ros2_java_tf.git
git clone https://github.com/wmlynar/ros2_java_launch.git
cd ros2_java_di
mvn clean install
cd ../ros2_java_di_example
mvn clean install
cd ../ros2_java_tf
mvn clean install
cd ../ros2_java_launch
mvn clean install</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers/>
  <buildWrappers/>
